            li(class="menu__item menu__item_active")
              img.menu__icon(src='../images/user.png' alt='user')
              span(class="menu__text menu__text_white") Customers
              img(class="menu__chevron menu__chevron_ml3" src='../images/chevronw.png' alt='chevron')
            li(class="menu__item menu__item_m")
              img.menu__icon(src='../images/wallet.png' alt='wallet')
